{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:import/recommended", "plugin:react/recommended"],
  "plugins": ["import", "react"],
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": false
  }
}
