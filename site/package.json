{
  "name": "redux-query-site",
  "private": true,
  "version": "3.0.0-alpha.8",
  "homepage": "https://amplitude.github.io/redux-query",
  "dependencies": {
    "babel-standalone": "^6.26.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.30.0",
    "github-markdown-css": "^2.5.0",
    "marked": "^0.3.6",
    "raw.macro": "^0.3.0",
    "react": "^16.8.2",
    "react-codemirror2": "^2.0.2",
    "react-dom": "^16.8.2",
    "react-inspector": "^2.2.0",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^4.0.0",
    "redux-query": "^3.0.0-alpha.8",
    "redux-query-interface-superagent": "^3.0.0-alpha.8",
    "redux-query-react": "^3.0.0-alpha.8",
    "rimraf": "^2.4.3",
    "styled-components": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "react-scripts": "^3.0.1"
  },
  "scripts": {
    "build": "yarn run vendor && react-scripts build",
    "clean": "rimraf build",
    "lint": "yarn run link && eslint src --fix --ext .js && eslint src --fix --ext .js.txt",
    "prebuild": "yarn run lint",
    "start": "yarn run link && yarn run vendor && react-scripts start",
    "link": "ln -f ../CHANGELOG.md src/CHANGELOG.md && ln -f ../README.md src/README.md",
    "vendor": "mkdir -p public/vendor && cp node_modules/redux-query/dist/umd/redux-query.js public/vendor/redux-query.js && cp node_modules/redux-query-react/dist/umd/redux-query-react.js public/vendor/redux-query-react.js && cp node_modules/redux-query-interface-superagent/dist/umd/redux-query-interface-superagent.js public/vendor/redux-query-interface-superagent.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
